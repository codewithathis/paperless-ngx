							"mode": "raw",
							"raw": "{\n    \"title\": \"Updated Document Title\",\n    \"correspondent\": 5,\n    \"document_type\": 2,\n    \"tags\": [1, 3],\n    \"storage_path\": 1,\n    \"archive_serial_number\": 12345\n}"
