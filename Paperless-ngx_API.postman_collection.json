							"raw": "{{base_url}}/api/paperless/documents/{{document_id}}/download?original=false",
							"host": [
								"{{base_url}}"
							],
							"path": [
								"api",
								"paperless",
								"documents",
								"{{document_id}}",
								"download"
							],
							"query": [
								{
									"key": "original",
									"value": "false",
									"description": "Download original file instead of processed version"
								}
							]
						},
						"description": "Download a document file"
					},
					"response": []
				}
			]
		},
